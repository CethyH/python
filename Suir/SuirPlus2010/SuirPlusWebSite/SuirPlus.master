    <style type="text/css">
        .alert-box {
            color: #555;
            border-radius: 10px;
            font-family: Tahoma,Geneva,Arial,sans-serif;
            font-size: 11px;
            padding: 10px 10px 10px 36px;
            margin: 10px;
            position: relative;
        }

        .notice {
            background: #fff8c4 url('images/warning_16.png') no-repeat 10px 50%;
            border: 1px solid #f2c779;
        }

        .alert-box span {
            font-weight: bold;
            text-transform: uppercase;
        }

        .tdevaluacion {
            padding: 5px;
        }

        .imagenmensaje {
            position: absolute;
            right: 0;
            bottom: 0;
            margin-right: 10px;
            margin-bottom: 10px;
        }
