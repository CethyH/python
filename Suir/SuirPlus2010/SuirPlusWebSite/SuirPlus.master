                <asp:LinkButton ID="lbMensajes" runat="server" CommandName="Mensaje" Visible="false" Font-Size="Medium" Font-Underline="true">Mensaje</asp:LinkButton>
                          
         
            </div>
            <uc1:UCSubEncabezadoEmp ID="UCSubEncabezadoEmp1" runat="server" />
