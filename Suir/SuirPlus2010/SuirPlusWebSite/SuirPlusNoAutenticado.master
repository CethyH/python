<body>
    <form id="form1" runat="server">
    <div id="header">
        <table cellspacing="0" cellpadding="0" width="100%">
            <tr>
                <td style="width: 10%;">
                    <asp:Image SkinID="logoSuir" ID="imgLogo" runat="server" />
                </td>
                <td style="width: 10%;">
                    <asp:Image SkinID="Gradiente" ID="imgGradiente" runat="server" />
                </td>
                <td style="width: 80%;" align="right">
                    <tss:ucInfoUsuario ID="UcInfoUsuario1" runat="server" />
                </td>
            </tr>
        </table>
    </div>
    <div id="navegation" align="left">
        <uc2:ucSuirPlusMenu ID="UcSuirPlusMenu1" runat="server" />
    </div>
    <div id="content">
        <asp:ContentPlaceHolder ID="MainContent" runat="server">
        </asp:ContentPlaceHolder>
    </div>
    <div id="footer">
        <b id="idDerechosReservados" runat="server"></b>
        <br />
        Ave. Tiradentes # 33, Tel: (809) 567-5049<br />
        <a href="http://www.tss.gov.do/terminos_uso_insert.htm">Términos de Uso</a> | <a
            href="http://www.tss.gov.do/politica_uso_insert.htm">Políticas </a>| <a href="http://www.tss.gov.do/faq_insert.htm">
                Preguntas Frecuentes</a>
        <br />
        <asp:Label ID="lblUsuarios" runat="server"></asp:Label>
        &nbsp;|
        <asp:Label ID="lblServer" runat="server"></asp:Label>
    </div>
    </form>
